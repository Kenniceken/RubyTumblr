<div class="mui-container-fluid">
  <h1>Latest Tags</h1>

  <div class="mui-row">
    <% @tags.each do |tag| %>
      <div class="mui-col-xs-12 mui-col-md-4 cardStyle">
          <div class="mui-panel my-3 text-center">
            <%= link_to tag.name, tag_path(tag), class: ' text-center post_title' %>
          </div>
      </div>
    <% end %>
  </div>
</div>
